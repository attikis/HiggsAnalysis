#!/bin/sh

#-- mH = 100
# observed limit
./LandS_AN/LandS/test/lands.exe -d HPlusHadronic_100.txt -M Hybrid  -v 1 --bQuickEstimateInitialLimit 0  --initialRmin 0 --initialRmax 0.3 | tail -5 >& output_LandS_100_temp_obs
head -3 output_LandS_100_temp_obs | tail -1 | awk '{print $12 }' >& output_LandS_100
#
# expected bands (ignore median)
./LandS_AN/LandS/test/lands.exe -d HPlusHadronic_100.txt -M Hybrid  -v 1 --bQuickEstimateInitialLimit 0  --initialRmin 0 --initialRmax 0.3 --doExpectation 1 -t  200 | tail -5 >& output_LandS_100_temp_exp
head -3 output_LandS_100_temp_exp | tail -1 | awk '{print $2 " " $3 " " $7 " " $5 " " $6}' >> output_LandS_100

# -- mH = 120
# observed limit
./LandS_AN/LandS/test/lands.exe -d HPlusHadronic_120.txt -M Hybrid  -v 1 --bQuickEstimateInitialLimit 0  --initialRmin 0 --initialRmax 0.3 | tail -5 >& output_LandS_120_temp_obs
head -3 output_LandS_120_temp_obs | tail -1 | awk '{print $12 }' >& output_LandS_120

# expected bands (ignore median)
./LandS_AN/LandS/test/lands.exe -d HPlusHadronic_120.txt -M Hybrid  -v 1 --bQuickEstimateInitialLimit 0  --initialRmin 0 --initialRmax 0.3 --doExpectation 1 -t  200 | tail -5 >& output_LandS_120_temp_exp
head -3 output_LandS_120_temp_exp | tail -1 | awk '{print $2 " " $3 " " $7 " " $5 " " $6}' >> output_LandS_120

# -- mH = 140
# observed limit
